- name: epy_block_0
  id: epy_block
  parameters:
    _source_code: "import numpy as np\nfrom gnuradio import gr\nimport os\n\nclass\
      \ cyclic_file_sink(gr.sync_block):\n    \"\"\"\n    Overwrites the first `vec_len`\
      \ floats of a file with each input vector.\n    Input vector length must match\
      \ `vec_len`.\n    \"\"\"\n    def __init__(self, filename=\"out.bin\", vec_len=256):\n\
      \        gr.sync_block.__init__(\n            self,\n            name=\"Cyclic\
      \ File Sink\",\n            in_sig=[(np.float32, vec_len)],  # fixed vector\
      \ of vec_len floats\n            out_sig=None\n        )\n        self.filename\
      \ = filename\n        self.vec_len = vec_len\n\n        # Create or truncate\
      \ file to vec_len floats\n        self.fh = open(self.filename, \"w+b\")\n \
      \       self.fh.write(b\"\\x00\" * (self.vec_len * 4))\n        self.fh.flush()\n\
      \n    def work(self, input_items, output_items):\n        in0 = input_items[0]\
      \  # shape: (#vectors, vec_len)\n\n        for vec in in0:\n            self.fh.seek(0)\n\
      \            self.fh.write(np.asarray(vec, dtype=np.float32).tobytes())\n  \
      \          self.fh.flush()\n\n        return len(in0)\n\n    def stop(self):\n\
      \        if self.fh:\n            self.fh.close()\n        return True\n"
    affinity: ''
    alias: ''
    comment: ''
    filename: '"/tmp/fft"'
    maxoutbuf: '0'
    minoutbuf: '0'
    vec_len: fft_size
  states:
    _io_cache: ('Cyclic File Sink', 'cyclic_file_sink', [('filename', "'out.bin'"),
      ('vec_len', '256')], [('0', 'float', 256)], [], '\n    Overwrites the first
      `vec_len` floats of a file with each input vector.\n    Input vector length
      must match `vec_len`.\n    ', ['filename', 'vec_len'])
